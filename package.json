{
  "name": "browser-vad",
  "version": "1.0.0",
  "description": "Client-side Voice Activity Detection using Silero VAD and ONNX Runtime Web",
  "type": "module",
  "main": "./dist/browser-vad.umd.js",
  "module": "./dist/browser-vad.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/browser-vad.es.js",
      "require": "./dist/browser-vad.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "vad",
    "voice-activity-detection",
    "silero",
    "onnx",
    "web-worker",
    "browser",
    "audio"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^1.0.0"
  },
  "dependencies": {
    "onnxruntime-web": "^1.16.0"
  }
}